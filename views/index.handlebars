                  <div class="category-tab" id={{this.id}}>
                    <h4>{{this.name}}</h4>
                  </div>
                </a>

                  <!-- Gallery Horizontal Scroll buttons -->
                  <div class="gallery-scroll-wrapper">
                    <a class="gallery-scroll-btn gallery-scroll-left">
